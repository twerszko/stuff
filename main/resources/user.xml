            <option name="JS_EXPRESSION" value="true"/>
            <option name="JSX_HTML" value="true"/>
            <option name="JS_STATEMENT" value="true"/>
        </context>
    </template>
    <template name="angular-service"
              value="(function (global) {&#10;    'use strict';&#10;&#10;    global.angular&#10;        .module('ncWebUiApp')&#10;        .service('$END$', [myService]);&#10;&#10;    function myService(){&#10;        return {&#10;&#10;        };&#10;    }&#10;&#10;})(this);"
