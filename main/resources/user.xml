<templateSet group="user">
    <template name="log" value=" private static final Logger LOG = LoggerFactory.getLogger($className$.class);"
              toReformat="false" toShortenFQNames="true">
        <variable name="className" expression="className()" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="JAVA_CODE" value="true"/>
            <option name="JAVA_STATEMENT" value="true"/>
            <option name="JAVA_EXPRESSION" value="true"/>
            <option name="JAVA_DECLARATION" value="true"/>
            <option name="JAVA_COMMENT" value="true"/>
            <option name="JAVA_STRING" value="true"/>
            <option name="COMPLETION" value="true"/>
        </context>
    </template>
    <template name="should"
              value="@Test&#10;public void should_() throws Exception {&#10;    // given&#10;    $END$&#10;            &#10;    // when&#10;    &#10;    &#10;    // then&#10;}"
              toReformat="false" toShortenFQNames="true">
        <context>
            <option name="JAVA_CODE" value="true"/>
            <option name="JAVA_STATEMENT" value="true"/>
            <option name="JAVA_EXPRESSION" value="true"/>
            <option name="JAVA_DECLARATION" value="true"/>
            <option name="JAVA_COMMENT" value="true"/>
            <option name="JAVA_STRING" value="true"/>
            <option name="COMPLETION" value="true"/>
        </context>
    </template>
    <template name="before" value="@Before&#10;public void setUp() {&#10;    $END$&#10;}" toReformat="false"
              toShortenFQNames="true">
        <context>
            <option name="JAVA_CODE" value="true"/>
            <option name="JAVA_STATEMENT" value="true"/>
            <option name="JAVA_EXPRESSION" value="true"/>
            <option name="JAVA_DECLARATION" value="true"/>
            <option name="JAVA_COMMENT" value="true"/>
            <option name="JAVA_STRING" value="true"/>
            <option name="COMPLETION" value="true"/>
